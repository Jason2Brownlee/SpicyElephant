.deck_title h3 { float: left; }
.left_content {	position: absolute;	margin-top: 50px; width: 260px; }
.right_content { clear: left; margin-left: 310px; }
.deck_title .deck_actions { height: 23px; line-height: 23px; margin: 10px 0 20px 10px; float: left; }
.deck_title .deck_actions a { font-size: 12px; }
.deck_description { font-size: 16px; margin-bottom: 10px; }
.deck_meta { font-size: 12px; margin-bottom: 10px; }
.deck_meta div { height: 20px; line-height: 16px; overflow: hidden; }
.deck_meta strong {  padding: 0; padding-left: 25px; color: #888; }
.deck_meta .count { background: url(../images/deck_cards.png) no-repeat; }
.deck_meta .public { background: url(../images/deck_unlocked.png) no-repeat; }
.deck_meta .private { background: url(../images/deck_locked.png) no-repeat; }
.users_panel, .comments_panel { border: 1px solid #dee0e6; padding: 1px; margin-bottom: 10px; }
.users_panel h4, .comments_panel h4 { text-indent: -9999px; margin-bottom: 5px; }
.details_link { margin: 10px 5px; font-size: 11px; display: block; }
.users_panel img { background: #fff; border: 1px solid #aaa; padding: 1px; float: left; margin-right: 5px; vertical-align: bottom; }
.users_panel p { font-size: 11px; margin-top: 2px; }
.users_panel .deck_author { background: #eff5f2; padding: 5px; overflow: auto; }
.users_panel .deck_author h4 { background: url(../images/deck_author.png) no-repeat; }
.users_panel .deck_author p.intro_text { color: #98a1b2; }
.users_panel .deck_users { padding: 5px; overflow: auto; }
.users_panel .deck_users h4 { background: url(../images/deck_topusers.png) no-repeat; }
.users_panel .user { overflow: auto; border-bottom: 1px solid #eee; padding: 5px 0; }
.users_panel .user:last-child { border: 0; }
.comments_panel .post_comment { background: #eff5f2; padding: 5px; overflow: auto; text-align: right; }
.comments_panel .post_comment h4 { background: url(../images/deck_comments.png) no-repeat; text-align: left; }
.comments_panel .post_comment textarea { width: 99%; border: 1px solid #dee0e6; }
.comment { overflow: auto; margin: 5px; font-size: 12px; }
.comment div { padding-left: 45px; }
.comment small { display: block; margin-top: 5px; }
.comment img { float: left;  background: #fff; border: 1px solid #aaa; padding: 1px; margin-right: 10px; }


  
