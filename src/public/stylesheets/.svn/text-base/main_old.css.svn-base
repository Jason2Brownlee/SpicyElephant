/* =ResetCSS (http://meyerweb.com/eric/tools/css/reset/)  */
html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, font, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend {	margin: 0; padding: 0; border: 0;	outline: 0;	font-size: 100%; vertical-align: baseline; background: transparent; }
body { line-height: 1; }
ol, ul { list-style: none; }
blockquote, q {	quotes: none; }
blockquote:before, blockquote:after,
q:before, q:after {	content: ''; content: none; }
:focus { outline: 0; }
ins {	text-decoration: none; }
del {	text-decoration: line-through; }

p { display: block; }
/* =MainLayout */
body {  font-family: 'Lucida Grande',Geneva,Arial,Verdana,sans-serif; text-align: center; font-size: 14px; color: #666; background: #cde2ec; }
.wrapper { position: relative; margin: 0 auto; max-width: 80%; min-width: 800px; text-align: left; border-top: none; }
.content_outer { border: 4px solid #adcddc; clear: both; height: 100%; margin-top: -6px;}
.content_inner { border: 1px solid #98b0bc; height: 100%; background: #fff; }
.footer { padding: 10px 0; text-align: center; margin: 0; }
.content { padding: 20px 10px 50px; height: 100%; }
/* =Headings */
h1 { padding: 10px 0 10px 5px; }
h2 { font-weight: normal; font-size: 120%; }
h3 { margin: 10px 0 20px -10px; overflow: auto; height: 25px; color: #fff; }
h3 span	{ float: left; display: block; min-width: 200px; padding: 0 10px; line-height: 25px; height: 25px; background: url(../images/h3_rounded.gif) no-repeat right #5991a6; }
/* =Navigation */
ul.nav_bar { position: relative; bottom: -8px; height: 35px; padding-left: 15px; }
ul.nav_bar li { float: left; padding-left: 10px; font-weight: 900; font-size: 110%; }
ul.nav_bar a { display: block; padding: 5px 15px 10px 5px; text-decoration: none; color: #96bbcd; }
ul.nav_bar a:hover { color: #779daf; }
ul.nav_bar li.current { background: url(../images/tab_left.png) no-repeat left top; }
ul.nav_bar li.current a { background: url(../images/tab_right.png) no-repeat right top; color: #779daf; }


.activity { margin-bottom: 7px; clear: left; }
.activity img { border: 1px solid #ccc; padding: 1px; float: left; margin-right: 5px; }
.activity div { float: left; clear: right; }
.activity .description { font-size: 90%; line-height: 1.2em; margin-bottom: 4px; }
.activity .actions { font-size: 80%; color: #777;}
.activity .actions a { background: none; padding: 0; }


img {
    border: none;
}
a, a:visited {
  color: #1291d3;
}
.login_links {
    position: absolute;
    top : 5px;
    right: 15px;
    color: #6dabbf;
}
.login_links a {
    color: #6dabbf;
}


.footer {
		font-size: .9em;
		line-height: 1.4em;
    color: #6dabbf; 
}
.footer a {
    color: #6dabbf;
    padding: 2px;
    margin-bottom: 10px;
}
.footer a:hover { 
    background: #4C95AF;
    color: #def1f2;
}
